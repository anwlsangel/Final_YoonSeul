<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <setting name="jdbcTypeForNull" value="NULL"/>
    </settings>
     <typeAliases>
    	<typeAlias type="com.kh.yoonsart.member.model.vo.Member" alias="member" />
    	<typeAlias type="com.kh.yoonsart.payment.model.vo.BuyList" alias="buylist" />
    	<typeAlias type="com.kh.yoonsart.concert.model.vo.Concert" alias="concert" />
    	<typeAlias type="com.kh.yoonsart.QNA.model.vo.QNA" alias="qna" />
    	<typeAlias type="com.kh.yoonsart.review.model.vo.Review" alias="review" />
    	<typeAlias type="com.kh.yoonsart.concert.model.vo.ConcertDate" alias="concertDate" />
    	<typeAlias type="com.kh.yoonsart.concert.model.vo.Ticket" alias="ticket" />

    </typeAliases>
    <mappers>
        <mapper resource="/mappers/member-mapper.xml"/>
        <mapper resource="/mappers/payment-mapper.xml"/>
        <mapper resource="/mappers/concert-mapper.xml"/>
        <mapper resource="/mappers/qna-mapper.xml"/>
        <mapper resource="/mappers/review-mapper.xml" />
    </mappers>
   
</configuration>


